      \strng{fullhashraw}{37f6b94ff52303cf740e5df3c9a5f074}
      \strng{bibnamehash}{37f6b94ff52303cf740e5df3c9a5f074}
      \strng{authorbibnamehash}{37f6b94ff52303cf740e5df3c9a5f074}
